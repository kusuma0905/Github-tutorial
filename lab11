        "def maximize_square_function(a, b):\n",
        "    # Check function values at the interval endpoints\n",
        "    f_a = a**2\n",
        "    f_b = b**2\n",
        "    # Find the maximum value between f(a) and f(b)\n",
        "    max_value = max(f_a, f_b)\n",
        "    return max_value\n",
        "\n",
        "# Example usage\n",
        "a = -5\n",
        "b = 3\n",
        "result = maximize_square_function(a, b)\n",
        "print(f\"The maximum value of f(x) = x^2 over the interval [{a}, {b}] is: {result}\")\n"
